{
  "name": "avidkit",
  "engines": {},
  "private": true,
  "repository": "https://gitlab.com/avidarvand/avidkit",
  "scripts": {
    "storybook": "run-s build:icons storybook:start",
    "storybook:start": "start-storybook -p 6006 -c .storybook",
    "build:icons": "turbo run build:dev --filter=@avidkit/icons",
    "build-storybook": "build-storybook -c .storybook -o storybook-build",
    "lint": "eslint '{components,tools}/**/*.{ts,tsx}'",
    "release": "yarn build -- --for-release && yarn changeset publish",
    "build": "turbo run build --concurrency 4",
    "purge-cache": "del \"./node_modules/.cache\" \"./**/dist\" \"./**/.turbo\" \"./**/_release\" \"!./**/node_modules\"",
    "update-versions": "changeset version && yarn install",
    "createNew": "bash create.sh"
  },
  "devDependencies": {
    "@avidkit/styles": "workspace:^",
    "@changesets/cli": "^2.22.0-temp.0",
